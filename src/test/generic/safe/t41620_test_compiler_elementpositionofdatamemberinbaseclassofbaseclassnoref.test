#>T.ulam
 ulam 5;
element T : System{

    Int test(){
    T2 t2;

    //relative baseclass pos to data member mRule
    Unsigned pos1 = t2.mRule.QBase.positionof;
    print(pos1);
    assert(pos1==62u); //includes element state bit offset 25

    //relative data member pos to data member mRule's pos
    Unsigned pos2 = t2.mRule.mInt.positionof;
    print(pos2);
    assert(pos2==66u); //includes element state bit offset 25

    //relative data member pos to data member mRule's BaseType
    Unsigned pos3 = t2.mRule.QBase.mInt.positionof;
    print(pos3);
    assert(pos3==4u);

    //relative data member pos to BaseType QB2
    Unsigned pos4 = t2.mRule.QB2.mInt.positionof;
    print(pos4);
    assert(pos4==1u);


    ChainRule tr;

    //absolute baseclass pos
    Unsigned rpos1 = tr.QBase.positionof;
    print(rpos1);
    assert(rpos1==62u);

    //relative data member position to BaseClass QBase
    Unsigned rpos2 = tr.QBase.mInt.positionof;
    print(rpos2);
    assert(rpos2==4u);

    //relative data member position to BaseClass QB2
    Unsigned rpos3 = tr.QB2.mInt.positionof; //mInt rel to QB2 == 1u
    print(rpos3);
    assert(rpos3==1u);

    //absolute data member position to stg
    Unsigned rpos4 = tr.mInt.positionof;
    print(rpos4);
    assert(rpos4==66u);

    //confirm additive property of positionof:
    assert(tr.QBase.positionof + tr.QBase.mInt.positionof == tr.mInt.positionof);

    return 0;
  }
}

