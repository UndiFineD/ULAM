##
## informed by talk 8/8/21 and 8/23/21 with Dave, series of of-Operators;
##    Special case feature: maxof classidof; sizeof classidof == log2 bits for maxof + 1
##
#>R.ulam
  ulam 5;
element R
{
  Unsigned cidsz = 0;
  Unsigned maxcid = 0;

  Int test() {
    System s;
    Unsigned mycid = A(Ele2.classidof).instanceof.classidof; //
    s.print(mycid);
    s.assert(mycid == 2u);

    maxcid = A(Ele2.classidof).instanceof.classidof.maxof;
    s.print(maxcid);
    s.assert(maxcid == 6u); //was Unsigned.maxof

    cidsz = A(Ele2.classidof).instanceof.classidof.sizeof; //sizeof==3bits
    s.print(cidsz);
    s.assert(cidsz == 3u);
    return 0;
  }
}

